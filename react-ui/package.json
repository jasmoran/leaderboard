{
  "name": "leaderboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link-http": "^1.5.3",
    "auth0-js": "^9.1.2",
    "graphql": "^0.13.0",
    "graphql-tag": "^2.8.0",
    "react": "^16.2.0",
    "react-apollo": "^2.1.3",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-typeahead": "^2.3.2",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build": "run-s build:*",
    "build:schema":  "apollo-codegen introspect-schema ../server/schema.graphql --output schema.json",
    "build:queries": "apollo-codegen generate src/components/*.tsx --schema schema.json --target typescript --output src/result-types.ts",
    "build:reactts": "react-scripts-ts build",
    "start": "run-p watch:*",
    "watch:schema":  "onchange '../server/schema.graphql' -- run-s build:schema build:queries",
    "watch:queries": "onchange 'src/components/*.tsx' -- npm run build:queries",
    "watch:reactts": "react-scripts-ts start"
  },
  "devDependencies": {
    "@types/auth0-js": "^8.11.2",
    "@types/graphql": "^0.12.5",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.34",
    "@types/react-bootstrap": "^0.32.2",
    "@types/react-bootstrap-typeahead": "^2.3.0",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-bootstrap": "^0.24.4",
    "@types/react-router-dom": "^4.2.3",
    "install": "^0.10.4",
    "npm": "^5.7.1",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "typescript": "^2.6.2"
  },
  "proxy": "http://localhost:3001/"
}
